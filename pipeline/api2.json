					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "POST",
					"url": "https://api.restful-api.dev/objects",
					"body": {
						"name": "Apple MacBook Pro 16",
						"data": {
							"year": 2019,
							"price": 1849.99,
							"CPU model": "Intel Core 19",
							"Hard disk size": "1 TB"
						}
					}
				}
			},
			{
				"name": "Copy data1",
